{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "projects": {
    "fixture": {
      "type": "application",
      "root": "src",
      "entryFile": "fixture-commands",
      "sourceRoot": "src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json"
      }
    }
  },
  "watchOptions": {
    "watchFile": "fixedPollingInterval"
  }
}